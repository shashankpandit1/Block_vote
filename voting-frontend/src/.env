# Backend API Configuration
REACT_APP_API_URL=http://localhost:4000

# Blockchain Configuration
BLOCKCHAIN_NETWORK=http://localhost:4000
BLOCKCHAIN_GENESIS_BLOCK=true
BLOCKCHAIN_DIFFICULTY=2
BLOCKCHAIN_MAX_VOTES=1000

# JWT or any secret keys (if applicable)
SECRET_KEY=your_secret_key_here

# Database (if you are using one)
DB_HOST=localhost
DB_PORT=27017
DB_NAME=voting_system

# For deployment, you can use cloud service credentials (if needed)
# AWS, Google Cloud, or any other provider keys
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Port for the server
PORT=4000

# Logging
LOG_LEVEL=debug

# Other useful environment variables for development
NODE_ENV=development
